node{
    // it will give the maven location
    def mvnHome=tool name: 'maven3', type: 'maven'
    stage("SCM Checkout"){
        git credentialsId: 'git',
        url: 'https://github.com/git212/jenkins.git',
        branch:'master'
    }
    stage('Maven Build'){
        sh script: "${mvnHome}/bin/mvn clean package"
    }
    stage('Maven deploy'){
        echo 'deploy is comming soon'
    }
}
