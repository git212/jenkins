pipeline{
    agent any
    parameters {
        string defaultValue: 'master', description: 'choose the branch to build and deploy', name: 'branchName', trim: false
    }
    stages{
        stage('scm checkout'){
            steps{
                git branch: "${params['branchName']}",
                    url: 'https://github.com/git212/jenkins'
            }        
        }
    }   
}